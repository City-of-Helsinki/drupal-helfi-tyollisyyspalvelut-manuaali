                "patches/elasticsearch_snakeoil_cert.patch": "patches/elasticsearch_snakeoil_cert.patch",
                "https://www.drupal.org/project/elasticsearch_connector/issues/3092486": "https://www.drupal.org/files/issues/2019-11-05/3092486.patch"
            },
            "drupal/group": {
                "https://www.drupal.org/project/group/issues/3340479": "https://www.drupal.org/files/issues/2023-02-09/3340479-1.patch",
                "Add support for 'view own unpublished entity' permission": "https://www.drupal.org/files/issues/2021-05-04/2876696-16.patch",
                "https://www.drupal.org/project/group/issues/2942657#comment-14535854": "https://www.drupal.org/files/issues/2022-05-25/group_route_ajax_route-2942657-27.patch"
