{
    "name": "drupal-composer/drupal-project",
    "description": "Project template for Drupal 9 projects with Composer",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "package",
            "package": {
              "name": "twbs/bootstrap-dist",
              "version": "5.1.3",
              "type": "drupal-library",
              "dist": {
                "url": "https://github.com/twbs/bootstrap/releases/download/v5.1.3/bootstrap-5.1.3-dist.zip",
                "type": "zip"
              }
            }
        },
        {
            "type": "package",
            "package": {
              "name": "citrussolutions/gulp-workflow",
              "version": "2.0.5",
              "type": "project",
              "dist": {
                "url": "https://github.com/CitrusSolutions/gulp-workflow/archive/v2.0.5.zip",
                "type": "zip"
              }
            }
        },
        {
          "type": "composer",
          "url": "https://asset-packagist.org"
        }
    ],
    "require": {
        "php": ">=7.3",
        "composer/installers": "^1.9",
        "cweagans/composer-patches": "^1.7",
        "drupal/address": "^1.10",
        "drupal/admin_toolbar": "^3.0",
        "drupal/auto_username": "^1.3",
        "drupal/autologout": "^1.4",
        "drupal/better_exposed_filters": "^5.0",
        "drupal/block_breakpoint": "^1.0@alpha",
        "drupal/block_class": "^2.0",
        "drupal/coder": "^8",
        "drupal/components": "^2.4",
        "drupal/conditional_fields": "^4.x-dev",
        "drupal/console": "^1.9",
        "drupal/content_access": "^1.0@alpha",
        "drupal/context": "^4.1",
        "drupal/core-composer-scaffold": "^9.1",
        "drupal/core-recommended": "~9.3",
        "drupal/csp": "^1.17",
        "drupal/date_recur": "^3.2",
        "drupal/date_recur_modular": "^3.0",
        "drupal/disable_html5_validation": "^1.1",
        "drupal/elasticsearch_connector": "^7.0@alpha",
        "drupal/emulsify_drupal": "^4.1",
        "drupal/emulsify_twig": "^2.0",
        "drupal/entity_diff_ui": "^1.0",
        "drupal/entity_print": "^2.4",
        "drupal/entitygroupfield": "^1.0@alpha",
        "drupal/eu_cookie_compliance": "^1.24",
        "drupal/eva": "^2.1",
        "drupal/facets": "^2.0",
        "drupal/field_group": "^3.2",
        "drupal/field_group_ajaxified_multipage": "^2.0",
        "drupal/field_permissions": "^1.1",
        "drupal/flag": "^4.0@beta",
        "drupal/flag_lists": "^4.0@RC",
        "drupal/gcontent_moderation": "^1.0@beta",
        "drupal/gdpr": "^3.0@beta",
        "drupal/geoblock": "^1.0",
        "drupal/geoblock_maxmind": "^1.0",
        "drupal/ggroup": "^1.0.0-alpha",
        "drupal/ginvite": "^2.0",
        "drupal/group": "^1.4",
        "drupal/hook_post_action": "^1.1",
        "drupal/ief_popup": "^2",
        "drupal/ief_table_view_mode": "^2.2",
        "drupal/inline_entity_form": "^1.0@RC",
        "drupal/log_stdout": "^1.3",
        "drupal/login_security": "^2.0",
        "drupal/mail_login": "^2.6",
        "drupal/markup": "^1.0@beta",
        "drupal/maxlength": "^2.0@RC",
        "drupal/menu_item_fields": "^1.5",
        "drupal/message": "^1.2",
        "drupal/message_notify": "^1.2",
        "drupal/message_ui": "^1.0@beta",
        "drupal/migrate_plus": "^6.0",
        "drupal/migrate_source_csv": "^3.5",
        "drupal/migrate_tools": "^6.0",
        "drupal/node_edit_protection": "^1.0",
        "drupal/nodeaccess": "^1.1",
        "drupal/notification_message": "^1.0@RC",
        "drupal/paragraphs": "^1.12",
        "drupal/password_policy": "^4.0",
        "drupal/pathauto": "^1.8",
        "drupal/radioactivity": "^3.1",
        "drupal/range": "^1.3",
        "drupal/real_aes": "^2.5",
        "drupal/recently_read": "^1.2",
        "drupal/redirect": "^1.7",
        "drupal/redis": "^1.5",
        "drupal/remove_http_headers": "^1.0",
        "drupal/require_on_publish": "^1.6",
        "drupal/search_api": "^1.23",
        "drupal/search_api_autocomplete": "^1.6",
        "drupal/search_api_solr": "4.x-dev",
        "drupal/search_api_solr_multilingual": "^3.1",
        "drupal/search_api_sorts": "^1.1",
        "drupal/search_api_sorts_widget": "^1.0@beta",
        "drupal/select2": "^1.13",
        "drupal/smart_date": "^3.5",
        "drupal/smtp": "^1.2",
        "drupal/svg_image": "^1.15",
        "drupal/tfa": "^1.0",
        "drupal/themable_forms": "^1.0@beta",
        "drupal/token": "^1.10",
        "drupal/twig_tweak": "^3.1",
        "drupal/view_password": "^6.0",
        "drupal/views_ef_fieldset": "1.x-dev",
        "drush/drush": "^10.3",
        "npm-asset/jquery-ui-touch-punch": "^0.2.3",
        "npm-asset/select2": "^4.0",
        "oomphinc/composer-installers-extender": "^2.0",
        "platformsh/config-reader": "^2.4",
        "twbs/bootstrap": "^5",
        "twbs/bootstrap-dist": "^5",
        "vlucas/phpdotenv": "^5.1",
        "webflo/drupal-finder": "^1.2",
        "weitzman/logintrait": "^1.2"
    },
    "require-dev": {
        "citrussolutions/gulp-workflow": "^2.0",
        "drupal/core-dev": "^9.3",
        "drupal/devel": "^4.1",
        "drupal/devel_kint_extras": "^1.0",
        "phpspec/prophecy-phpunit": "^2.0",
        "weitzman/drupal-test-traits": "^1.5",
        "zaporylie/composer-drupal-optimizations": "^1.2"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "discard-changes": true,
        "sort-packages": true,
        "process-timeout":0,
        "allow-plugins": {
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "drupal/core-composer-scaffold": true,
            "zaporylie/composer-drupal-optimizations": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "oomphinc/composer-installers-extender": true,
            "drupal/console-extend-plugin": true
        }
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": ["load.environment.php"]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "public/"
            }
        },
        "installer-paths": {
            "public/core": ["type:drupal-core"],
            "public/libraries/{$name}": [
                "type:drupal-library",
                "vendor:npm-asset",
                "vendor:bower-asset"
            ],
            "public/modules/contrib/{$name}": ["type:drupal-module"],
            "public/profiles/contrib/{$name}": ["type:drupal-profile"],
            "public/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/contrib/{$name}": ["type:drupal-drush"]
        },
        "installer-types": ["bower-asset", "npm-asset"],
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "patches": {
            "drupal/core": {
                "https://www.drupal.org/project/drupal/issues/2800875#comment-14070351": "https://www.drupal.org/files/issues/2021-04-23/2800875-70_0.patch",
                "https://www.drupal.org/project/drupal/issues/1038316#comment-14308196": "https://www.drupal.org/files/issues/2021-12-01/1038316-242.patch"
            },
            "drupal/message": {
                "https://www.drupal.org/files/issues/2018-06-24/allow_token_contexts-3.patch": "https://www.drupal.org/files/issues/2018-06-24/allow_token_contexts-3.patch"
            },
            "drupal/notification_message": {
                "https://www.drupal.org/files/issues/2022-03-24/3270975_4_add_notification_msgs_to_views.patch": "https://www.drupal.org/files/issues/2022-03-24/3270975_4_add_notification_msgs_to_views.patch"
            },
            "drupal/elasticsearch_connector": {
                "https://www.drupal.org/project/elasticsearch_connector/issues/2912308#comment-14000645": "https://www.drupal.org/files/issues/2021-02-16/partial-search-issue-2912308-14.patch",
                "patches/elasticsearch_snakeoil_cert.patch": "patches/elasticsearch_snakeoil_cert.patch",
                "https://www.drupal.org/project/elasticsearch_connector/issues/3092486": "https://www.drupal.org/files/issues/2019-11-05/3092486.patch"
            },
            "drupal/ggroup": {
                "https://www.drupal.org/project/ggroup/issues/3084153": "./patches/3084153-role-mapper.patch"
            },
            "drupal/field_group_ajaxified_multipage": {
                "https://www.drupal.org/project/field_group_ajaxified_multipage/issues/3280612#comment-14515962" : "https://www.drupal.org/files/issues/2022-05-16/3280612-duplicate-core-version-requirement-1.patch"
            },
            "drupal/themable_forms": {
                "https://www.drupal.org/files/issues/undefined_index_type_themable_forms_theme_suggestions_form_element_2903660.patch": "https://www.drupal.org/files/issues/undefined_index_type_themable_forms_theme_suggestions_form_element_2903660.patch"
            },
            "drupal/nodeaccess": {
                "https://www.drupal.org/project/nodeaccess/issues/3169490#comment-13814377": "https://www.drupal.org/files/issues/2020-09-07/nodeaccess_unpublish_nodes_visible-3169490-3.patch"
            }
        }
    }
}
