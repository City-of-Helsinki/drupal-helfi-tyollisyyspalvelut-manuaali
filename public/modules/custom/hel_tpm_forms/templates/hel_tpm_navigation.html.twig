{% block content %}
<div class='nav page-controls'>
  <div class='button-wrapper'>
    <button class='button btn-prev' type="button" id="prevBtn">Previous</button>
    <button class='button btn-next' type="button" id="nextBtn">Next</button>
  </div>
</div>
{% endblock %}
